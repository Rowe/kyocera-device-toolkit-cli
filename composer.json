{
  "name": "Printer Helper",
  "description": "Utility for printers",
  "keywords": [
    "printer"
  ],
  "homepage": "https://github.com/Rowe/printer-helper",
  "type": "project",
  "license": "MIT license",
  "support": {
    "issues": "https://github.com/rowe/printer-helper/issues?state=open",
    "source": "https://github.com/rowe/printer-helper"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0"
  },
  "config": {
    "process-timeout": 999999
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "autoload": {
    "psr-4": {
      "rowe\\printerHelper\\": "src"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  }
}
